{
    "name": "skylence/filament-system-configuration",
    "description": "A flexible system configuration management plugin for Filament 5 with sidebar navigation.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "filament/filament": "^5.0",
        "skylence/filament-context-sidebar": "*",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Skylence\\FilamentSystemConfiguration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skylence\\FilamentSystemConfiguration\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse",
        "rector": "vendor/bin/rector process"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skylence\\FilamentSystemConfiguration\\FilamentSystemConfigurationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
